{
    "binary_output_path": "epa21_model.out",
    "input_template_path": "epa21_model.tpl",
    "input_parameters_path": "epa21_lid_input_parameters.json",
    "input_path": "epa21_model.inp",
    "report_output_path": "epa21_model.rpt",
    "summary_dir": ".",
    "swmm_path": "./swmm5.sh",
    "extract": {
        "steps": [
            {
                "type": "node",
                "enabled": true,
                "output_path": "epa21_model_nodes.csv",
